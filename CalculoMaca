public class CalculoMaca{
  
  public static void main(String args[]){
    
        int    quantidade = 30;
        float  total;

        if (quantidade >= 12) {
          total = Calcula25(quantidade);
        }else{
          total = Calcula30(quantidade);     
        }
    
       System.out.println(total);
       
  }
  
  public static float Calcula30(int qtd){
    
     float  preco30    = 0.30f;
     return qtd * preco30;
  }
  
   public static float Calcula25(int qtd){
    
      float  preco25    = 0.25f;
      return qtd * preco25;
  }
}
